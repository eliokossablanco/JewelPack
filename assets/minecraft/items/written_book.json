        "model": {
          "type": "minecraft:model",
          "model": "item/title_of_proficiency"
        }
      },
      {
        "when": "Title of Design",
        "model": {
          "type": "minecraft:model",
          "model": "item/title_of_design"
        }
      },
      {
        "when": "Title of Mastery",
        "model": {
          "type": "minecraft:model",
          "model": "item/title_of_mastery"
        }
      },
      {
        "when": "Proof of Mastery",
